<div class="form-group row">
    <div class="col-6">
        <label>First Name</label>
        <input type="text" name="first_name" class="form-control" placeholder="First Name">
    </div>
    <div class="col-6">
        <label>Last Name</label>
        <input type="text" name="last_name" class="form-control" placeholder="Last Name">
    </div>
</div>
<div class="form-group row">
    <div class="col-4">
        <label>Email</label>
        <input type="email" name="email" class="form-control" parsley-trigger="change" placeholder="Email">
    </div>
    <div class="col-4">
        <label>Password</label>
        <input type="password" name="password" id="password" class="form-control" placeholder="Password">
    </div>
    <div class="col-4">
        <label>Confirm Password</label>
        <input type="password" name="confirm_password" class="form-control" placeholder="Confirm Password">
    </div>
</div>
<div class="form-group row">
    <div class="col-4">
        <label>Mobile</label>
        <input type="text" name="mobile_no" class="form-control" placeholder="Mobile">
    </div>
    <div class="col-4">
        <label>Gender</label>
        <select name="gender" class="form-control">
            <option value="">Select</option>
            <option value="0">Male</option>
            <option value="1">Female</option>
            <option value="2">Other</option>
        </select>
    </div>
    <div class="col-4">
        <label>Date of Birth</label>
        <input type="text" name="dob" class="form-control"  placeholder="Date of Birth">
    </div>
</div>
<div class="form-group row">
    <div class="col-4">
        <label>Country</label>
        <select name="country" class="form-control">
            <option value="">Select</option>
            <option>India</option>
            <option>Nepal</option>
            <option>America</option>
        </select>
    </div>
    <div class="col-4">
        <label>State</label>
        <select name="state" class="form-control">
            <option value="">Select</option>
            <option>Delhi</option>
            <option>Punjab</option>
            <option>Haryana</option>
        </select>
    </div>
    <div class="col-4">
        <label>City</label>
        <select name="city" class="form-control">
            <option value="">Select</option>
            <option>Mohali</option>
            <option>SAS Nagar</option>
            <option>Suhana</option>
        </select>
    </div>
</div>
<div class="form-group row">
    <div class="col-9">
        <label>Address</label>
        <input type="text" name="address" class="form-control" placeholder="Address">
    </div>
    <div class="col-3">
        <label>Pin Code</label>
        <input type="text" name="pin_code" class="form-control"  placeholder="Pin Code">
    </div>
</div>
<div class="form-group row">
    <div class="col-12">
        <label>Landmark</label>
        <input type="text" name="landmark" class="form-control"  placeholder="Landmark">
    </div>
</div>
<div class="form-group row dragDrop">
    <div class="col-12">
        <label>Image</label>
        <div class="row m-0">
            <div class="dropzone-wrapper col-8">
                <div class="dropzone-desc">
                <i class="fa fa-cloud-upload"></i>
                <p>Choose an image file or drag it here.</p>
                </div>
                <input type="file" name="image" class="dropzone">
            </div>
            <div class="preview-zone hidden col-4">
                <div class="box box-solid">
                    <div class="position-absolute" style="right: 0;">
                        <button style="display: none;" type="button" class="btn btn-danger btn-xs remove-preview">
                            <i class="fa fa-times"></i> Remove
                        </button>
                    </div>
                <div class="box-body"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="form-group row">
    <div class="col-12 mt-2">
        <label>Status</label>
        <input type="checkbox" name="status" checked data-plugin="switchery" data-color="#1bb99a"/>
    </div>
</div>
